{
  "private": true,
  "workspaces": [
    "AccountCenter",
    "AccountSettings",
    "DashboardAnalysis",
    "DashboardMonitor",
    "DashboardWorkplace",
    "EditorFlow",
    "EditorKoni",
    "EditorMind",
    "EmptyPage",
    "Exception403",
    "Exception404",
    "Exception500",
    "FormAdvancedForm",
    "FormBasicForm",
    "FormStepForm",
    "ListBasicList",
    "ListCardList",
    "ListSearch",
    "ListSearchApplications",
    "ListSearchArticles",
    "ListSearchProjects",
    "ListTableList",
    "ProfileAdvanced",
    "ProfileBasic",
    "ResultFail",
    "ResultSuccess",
    "UserRegister",
    "UserRegisterResult"
  ],
  "scripts": {
    "postinstall": "max setup",
    "lint": "npm run lint:js && tsc --noEmit",
    "lint-staged": "lint-staged && node ./.script/genBlockConfig.js&& git add ./umi-block.json",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty  ./",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "prettier": "prettier -c --write \"**/*\"",
    "screenshot": "pro screenshot",
    "start": "max dev",
    "dev": "max dev EmptyPage",
    "test": "max test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-cli": "^1.0.5",
    "@ant-design/pro-components": "^2.6.43",
    "@types/express": "^4.17.13",
    "@umijs/max": "4.0.89",
    "antd": "^5.11.5",
    "antd-style": "^3.5.2",
    "lodash.debounce": "^4.0.8",
    "numeral": "^2.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mockjs": "^1.0.10",
    "@types/numeral": "^0.0.28",
    "@types/react": "18.2.42",
    "@types/react-dom": "^17.0.25",
    "cross-env": "^7.0.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.0",
    "lint-staged": "^10.0.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.4",
    "typescript": "5.3.2"
  },
  "checkFiles": [
    "**/*.ts*",
    "**/*.less",
    "**/*.md"
  ]
}
